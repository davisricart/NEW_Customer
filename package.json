{
  "name": "gr-balance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run kill-ports && npm run dev",
    "dev:force": "npm run kill-ports && vite --force",
    "kill-ports": "npx kill-port 5177 5178 5179 || echo 'No processes to kill'",
    "start": "npm run dev:clean",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "download": "node scripts/download.js",
    "setup": "node scripts/setup.js",
    "rename": "node scripts/setup.js",
    "create-instance": "node scripts/create-instance.js",
    "typecheck": "tsc --noEmit",
    "cleanup": "node scripts/cleanup.js",
    "setup-github": "node scripts/setup-github.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.3.0",
    "@emailjs/browser": "^3.11.0",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.9.0",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "form-data": "^4.0.2",
    "lambda-multipart-parser": "^1.0.1",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.0",
    "pm2": "^6.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.12",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.21.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.6",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.16",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
